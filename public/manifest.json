{
  "short_name": "FACT App",
  "name": "Faceit Analysis Counter Teams",
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "./static/js/content.js"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://open.faceit.com/*",
    "https://api.faceit.com/*"
  ]
}